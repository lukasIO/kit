{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"noImplicitAny": true,
		"target": "es2020",
		"module": "es2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"paths": {
			"types": ["./types"],
			"types.internal": ["./types.internal"]
		}
	},
	"include": ["src/**/*"]
}
